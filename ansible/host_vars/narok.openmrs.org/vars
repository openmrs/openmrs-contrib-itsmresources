   - openmrs.qa2.openconceptlab.org

nginx_keepalive_timeout: "600"
nginx_keepalive_requests: "600"

nginx_vhosts:
  - listen: "80 default_server"
    server_name: "qa.openconceptlab.org"
    filename: "qa.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "qa.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclweb_access.log;
      error_log /var/log/nginx/oclweb_error.log;
      ssl_certificate /etc/letsencrypt/live/narok.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/narok.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:7000/;
      }
  - listen: "80"
    server_name: "api.qa.openconceptlab.org"
    filename: "api.qa.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "api.qa.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclapi_access.log;
      error_log /var/log/nginx/oclapi_error.log;
      ssl_certificate /etc/letsencrypt/live/narok.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/narok.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        set $cors '';
        if ($http_origin ~ '^https?://(localhost|.*\.openconceptlab\.org)') {
            set $cors 'true';
        }

        if ($cors = 'true') {
            add_header 'Access-Control-Allow-Origin' "$http_origin" always;
            add_header 'Access-Control-Allow-Credentials' 'true' always;
            add_header 'Access-Control-Allow-Methods' 'GET, POST, PUT, DELETE, OPTIONS' always;
            add_header 'Access-Control-Allow-Headers' 'Accept,Authorization,Cache-Control,Content-Type,DNT,If-Modified-Since,Keep-Alive,Origin,User-Agent,X-Requested-With,Range,num_found,num_returned,offset' always;
            add_header 'Access-Control-Expose-Headers' 'Content-Length,Content-Range' always;
            # required to be able to read Authorization header in frontend
            #add_header 'Access-Control-Expose-Headers' 'Authorization' always;
        }

        if ($request_method = 'OPTIONS') {
            # Tell client that this pre-flight info is valid for 20 days
            add_header 'Access-Control-Max-Age' 1728000;
            add_header 'Content-Type' 'text/plain charset=UTF-8';
            add_header 'Content-Length' 0;
           return 204;
        }
        proxy_pass http://127.0.0.1:8000/;
      }
  - listen: "80"
    server_name: "flower.qa.openconceptlab.org"
    filename: "flower.qa.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "flower.qa.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclapi_access.log;
      error_log /var/log/nginx/oclapi_error.log;
      ssl_certificate /etc/letsencrypt/live/narok.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/narok.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:5555/;
      }
  - listen: "80"
    server_name: "openmrs.qa.openconceptlab.org"
    filename: "openmrs.qa.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
