datadog_tags_extra:
  - "service:openmrs-refapp"
  - "service:openmrs-platform"
  - "service:ocl"
  - "service:formbuilder"

docker_deployment:
 - demo
 - mdsbuilder
 - emr-3-demo
 - oclclient-prd
 - cieladmin
 - formbuilder

letsencrypt_cert_domains:
  - bele.openmrs.org
  - cieladmin.openmrs.org
  - demo.openmrs.org
  - mdsbuilder.openmrs.org
  - o3.openmrs.org
  - openmrs.openconceptlab.org
  - formbuilder.o3.openmrs.org


nginx_keepalive_timeout: "600"
nginx_keepalive_requests: "600"

nginx_vhosts:
  - listen: "80 default_server"
    server_name: "demo.openmrs.org"
    filename: "demo.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "demo.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/demo_access.log;
      error_log /var/log/nginx/demo_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        return 301 https://demo.openmrs.org/openmrs;
      }
      location /openmrs {
        proxy_set_header HOST $host;
        proxy_set_header  X-Forwarded-Proto $scheme;
        proxy_pass http://127.0.0.1:8080/openmrs;
      }
  - listen: "80"
    server_name: "mdsbuilder.openmrs.org"
    filename: "mdsbuilder.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "mdsbuilder.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/mdsbuilder_access.log;
      error_log /var/log/nginx/mdsbuilder_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        return 301 https://mdsbuilder.openmrs.org/openmrs;
      }
      location /openmrs {
        proxy_set_header HOST $host;
        proxy_set_header  X-Forwarded-Proto $scheme;
        proxy_pass http://127.0.0.1:8081/openmrs;
      }
  - listen: "80"
    server_name: "o3.openmrs.org"
    filename: "o3.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "o3.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/o3_access.log;
      error_log /var/log/nginx/o3_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_set_header HOST $host;
        proxy_set_header  X-Forwarded-Proto $scheme;
        proxy_set_header  X-Real-IP $remote_addr;
        proxy_set_header  X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_pass http://127.0.0.1:8085/;
      }
  - listen: "80"
    server_name: "openmrs.openconceptlab.org"
    filename: "openmrs.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "openmrs.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclclient_access.log;
      error_log /var/log/nginx/oclclient_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:8083/;
      }
  - listen: "443 ssl"
    server_name: "bele.openmrs.org"
    extra_parameters: |
     access_log /var/log/nginx/deployer_access.log;
     error_log /var/log/nginx/deployer_error.log;
     ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
     ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
     location ^~ /.well-known/acme-challenge/ {
       root /usr/share/nginx/html;
     }
  - listen: "80"
    server_name: "cieladmin.openmrs.org"
    filename: "cieladmin.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "cieladmin.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/cieladmin_access.log;
      error_log /var/log/nginx/cieladmin_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        return 301 https://cieladmin.openmrs.org/openmrs;
      }
      location /openmrs {
        proxy_set_header HOST $host;
        proxy_set_header  X-Forwarded-Proto $scheme;
        proxy_pass http://127.0.0.1:8084/openmrs;
      }
  - listen: "80"
    server_name: "formbuilder.o3.openmrs.org"
    filename: "formbuilder.o3.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "formbuilder.o3.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/formbuilder_o3_access.log;
      error_log /var/log/nginx/formbuilder_o3_error.log;
      ssl_certificate /etc/letsencrypt/live/bele.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/bele.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_set_header HOST $host;
        proxy_set_header  X-Forwarded-Proto $scheme;
        proxy_set_header  X-Real-IP $remote_addr;
        proxy_set_header  X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_pass http://127.0.0.1:8087/;
      }

