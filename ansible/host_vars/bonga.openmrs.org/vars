    server_name: "qa-refapp2.openmrs.org"
    filename: "qa-refapp2.openmrs.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "qa-refapp2.openmrs.org"
    extra_parameters: |
