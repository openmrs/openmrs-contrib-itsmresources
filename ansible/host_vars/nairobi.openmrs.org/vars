letsencrypt_cert_domains:
   - nairobi.openmrs.org
   - www.openconceptlab.org
   - openconceptlab.org
   - api.openconceptlab.org
   - flower.openconceptlab.org
   - openmrs.openconceptlab.org
   - msp.openconceptlab.org

nginx_keepalive_timeout: "600"
nginx_keepalive_requests: "600"

nginx_vhosts:
  - listen: "80 default_server"
    server_name: "www.openconceptlab.org openconceptlab.org"
    filename: "www.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "www.openconceptlab.org openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclweb_access.log;
      error_log /var/log/nginx/oclweb_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:7000/;
      }
  - listen: "80"
    server_name: "api.openconceptlab.org"
    filename: "api.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "api.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclapi_access.log;
      error_log /var/log/nginx/oclapi_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        if ($request_method = 'OPTIONS') {
          add_header 'Access-Control-Allow-Origin' '*';
          add_header 'Access-Control-Allow-Methods' 'GET, OPTIONS, POST, PUT, DELETE';
          #
          # Custom headers and headers various browsers *should* be OK with but aren't
          #
          add_header 'Access-Control-Allow-Headers' 'num_found,num_returned,offset,authorization,DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range';
          #
          # Tell client that this pre-flight info is valid for 20 days
          #
          add_header 'Access-Control-Max-Age' 1728000;
          add_header 'Content-Type' 'text/plain; charset=utf-8';
          add_header 'Content-Length' 0;
          return 204;
        }
        if ($request_method = 'POST') {
          add_header 'Access-Control-Allow-Methods' 'GET, OPTIONS, POST, PUT, DELETE';
          add_header 'Access-Control-Allow-Headers' 'num_found,num_returned,offset,authorization,DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range';
          add_header 'Access-Control-Expose-Headers' 'Content-Length,Content-Range';
        }
        if ($request_method = 'PUT') {
          add_header 'Access-Control-Allow-Methods' 'GET, OPTIONS, POST, PUT, DELETE';
          add_header 'Access-Control-Allow-Headers' 'num_found,num_returned,offset,authorization,DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range';
          add_header 'Access-Control-Expose-Headers' 'Content-Length,Content-Range';
        }
        if ($request_method = 'GET') {
          add_header 'Access-Control-Allow-Methods' 'GET, OPTIONS, POST, PUT, DELETE';
          add_header 'Access-Control-Allow-Headers' 'num_found,num_returned,offset,authorization,DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range';
          add_header 'Access-Control-Expose-Headers' 'Content-Length,Content-Range';
        }
        if ($request_method = 'DELETE') {
          add_header 'Access-Control-Allow-Methods' 'GET, OPTIONS, POST, PUT, DELETE';
          add_header 'Access-Control-Allow-Headers' 'num_found,num_returned,offset,authorization,DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range';
          add_header 'Access-Control-Expose-Headers' 'Content-Length,Content-Range';
        }
        proxy_pass http://127.0.0.1:8000/;
      }
  - listen: "80"
    server_name: "flower.openconceptlab.org"
    filename: "flower.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "flower.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclapi_access.log;
      error_log /var/log/nginx/oclapi_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:5555/;
      }
  - listen: "80"
    server_name: "openmrs.openconceptlab.org"
    filename: "openmrs.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "openmrs.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclclient_access.log;
      error_log /var/log/nginx/oclclient_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:8081/;
      }
  - listen: "80"
    server_name: "msp.openconceptlab.org"
    filename: "msp.openconceptlab.org.80.conf"
    extra_parameters: |
      return 301 https://$host$request_uri;
  - listen: "443 ssl"
    server_name: "msp.openconceptlab.org"
    extra_parameters: |
      access_log /var/log/nginx/oclmsp_access.log;
      error_log /var/log/nginx/oclmsp_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:8082/;
      }
  - listen: "443 ssl"
    server_name: "nairobi.openmrs.org"
    extra_parameters: |
      access_log /var/log/nginx/deployer_access.log;
      error_log /var/log/nginx/deployer_error.log;
      ssl_certificate /etc/letsencrypt/live/nairobi.openmrs.org/fullchain.pem;
      ssl_certificate_key /etc/letsencrypt/live/nairobi.openmrs.org/privkey.pem;
      location ^~ /.well-known/acme-challenge/ {
        root /usr/share/nginx/html;
      }
      location / {
        proxy_pass http://127.0.0.1:3000/;
      }

